@prefix : <http://kalnassag.com/onepiece#> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

: a owl:Ontology ;
    rdfs:comment "One Piece World Ontology. This is a ontology representing the world of One Piece, including characters, locations, devil fruits, and other relevant entities. this is project is not affiliated with any official One Piece media owners or organizations and all original copyrights belong to the respective owners";
     .


###############################################
##### Classes and properties                        
###############################################

:Character a owl:Class ;
    rdfs:subClassOf dbo:FictionalCharacter ;
    rdfs:label "One Piece Character"@en ;
    rdfs:label "ワンピースのキャラクター"@ja ;
    rdfs:comment "A character in the One Piece universe."@en ;
    rdfs:comment "ワンピースの世界に登場するキャラクター。"@ja ;
    .

:romanizedName a owl:DatatypeProperty ;
    rdfs:label "Romanized Name"@en ;
    rdfs:comment "The romanized name of a character" ;
    rdfs:domain :Character ;
    rdfs:range xsd:string .

:Affiliation a owl:Class ;
    rdfs:label "Affiliation" ;
    rdfs:comment "An entity with which a character or a group of characters affiliates" .

:Occupation a owl:Class ;
    rdfs:label "Occupation" ;
    rdfs:comment "The occupation of a character in the One Piece universe. e.g. Pirate Captain, Cook, Pirate Officer, Marine Admiral" .

:Pirate a owl:Class ;
    rdfs:label "Pirate" ;
    rdfs:comment "A pirate character in the One Piece universe." ;
    rdfs:subClassOf :Affiliation .

:Marine a owl:Class ;
    rdfs:label "Marine" ;
    rdfs:comment "A Marine/Navy character in the One Piece universe." ;
    rdfs:subClassOf :Affiliation .

:DevilFruit a owl:Class ;
    rdfs:label "Devil Fruit" ;
    rdfs:comment "A devil fruit found in the One Piece universe".

:Status a owl:Class ;
    rdfs:label "Status" ;
    rdfs:comment "The current status of a character (alive, deceased, missing, etc.)" .

:PirateCrew a owl:Class, schema:Organization;
    rdfs:label "Pirate Crew" ;
    rdfs:comment "A group of pirates flying under the same jolly roger led by the same captain".

:Alliance a owl:Class ;
    rdfs:label "Alliance" ;
    rdfs:comment "An alliance formed by different groups/factions";
    rdfs:example "The alliance between the Starw Hats and the Hearts Pirates".

:Region a owl:Class ;
    rdfs:label "Region" ;
    rdfs:comment "A region in the world of One Piece, consisting of multiple kingdoms and islands" ;
    rdfs:subClassOf :Location .

:Sea a owl:Class ;
    rdfs:label "Sea" ;
    rdfs:comment "A sea in the world of One Piece" ;
    rdfs:subClassOf :Location .

:Kingdom a owl:Class ;
    rdfs:label "Kingdom" ;
    rdfs:comment "A kingdom in the world of One Piece, not the same as the island on which the kingdom exists".

:citizenOf a owl:ObjectProperty ;
    rdfs:label "Citizen Of" ;
    rdfs:comment "The kingdom of which the character is a citizen" ;
    rdfs:domain :Character ;
    rdfs:range :Kingdom .
:Island a owl:Class ;
    rdfs:label "Island" ;
    rdfs:comment "An island in the world of One Piece" ;
    rdfs:subClassOf :Location .

:City a owl:Class ;
    rdfs:label "City" ;
    rdfs:comment "A city in the One Piece universe" ;
    rdfs:subClassOf :Location .

:Location a owl:Class ;
    rdfs:label "Location" ;
    rdfs:comment "A location in the One Piece universe, such as a building or landmark" .

:Creature a owl:Class ;
    rdfs:label "Creature" ;
    rdfs:comment "A non-speaking creature in the One Piece universe" .

:Race a owl:Class ;
    rdfs:label "Race" ;
    rdfs:comment "Races, sometimes referred to as Tribes, are groups of sentient beings that share distinctive physical traits. Many races coexist in the world, with some originating from the moon and beyond." .

:DevilFruitType a owl:Class ;
    rdfs:label "Devil Fruit Type" ;
    rdfs:comment "The type of devil fruit (Paramecia, Zoan, or Logia)" .

:Arc a owl:Class ;
    rdfs:label "Story Arc" ;
    rdfs:comment "A story arc in the One Piece series" ;.

:MangaIssue a owl:Class ;
    rdfs:label "Manga Issue" ;
    rdfs:comment "A manga issue of the One Piece series" ;.

:AnimeEpisode a owl:Class ;
    rdfs:label "Anime Episode" ;
    rdfs:comment "An anime episode of the One Piece series" ;.

:Bounty a owl:Class;
rdfs:label "Bounty" ;
rdfs:comment "The bounty on a pirate's head in berries".

:BloodType a owl:Class ;
    rdfs:label "Blood Type" ;
    rdfs:comment "The blood type of a character" .

:liveActionPortrayedBy a owl:ObjectProperty ;
    rdfs:label "Live Action Portrayed By" ;
    rdfs:comment "The actor who portrayed the character in the live-action adaptation" ;
    rdfs:domain :Character ;
    rdfs:range foaf:Person .
###############################################
##### Properties                             
###############################################

###############################################
##### Object Properties                             
###############################################

### Character Properties ###
:residence a owl:ObjectProperty ;
    rdfs:label "Residence" ;
    rdfs:comment "The place where a character currently resides" ;
    rdfs:domain :Character ;
    rdfs:range :Location .

:hasBloodType a owl:ObjectProperty ;
    rdfs:label "Has Blood Type" ;
    rdfs:comment "The blood type of a character" ;
    rdfs:domain :Character ;
    rdfs:range :BloodType .

:japaneseVoiceActor a owl:ObjectProperty ;
    rdfs:label "Japanese Voice Actor" ;
    rdfs:comment "The Japanese voice actor for a character" ;
    rdfs:domain :Character ;
    rdfs:range foaf:Person .

:englishVoiceActor a owl:ObjectProperty ;
    rdfs:label "English Voice Actor" ;
    rdfs:comment "The English voice actor for a character" ;
    rdfs:domain :Character ;
    rdfs:range foaf:Person .


:animeDebut a owl:ObjectProperty ;
    rdfs:label "Anime Debut" ;
    rdfs:comment "The anime episode in which the character first appeared" ;
    rdfs:domain :Character ;
    rdfs:range :AnimeEpisode .

:mangaDebut a owl:ObjectProperty ;
    rdfs:label "Manga Debut" ;
    rdfs:comment "The manga issue in which the character first appeared" ;
    rdfs:domain :Character ;
    rdfs:range :MangaIssue .

:occupation a owl:ObjectProperty ;
    rdfs:label "Occupation" ;
    rdfs:comment "The occupation of a character in the One Piece universe" ;
    rdfs:domain :Character ;
    rdfs:range :Occupation .

:statusOfCharacter a owl:ObjectProperty ;
    rdfs:label "Status of Character" ;
    rdfs:comment "The current status of a character (alive, deceased, missing, etc.)" ;
    rdfs:domain :Character ;
    rdfs:range :Status .

:affiliatesWith a owl:ObjectProperty ;
    rdfs:label "Affiliates With" ;
    rdfs:comment "The affiliation of a character or group of characters" ;
    rdfs:domain :Character ;
    rdfs:range :Affiliation .

:hasDevilFruit a owl:ObjectProperty ;
    rdfs:label "Has Devil Fruit" ;
    rdfs:comment "The devil fruit that a character has eaten" ;
    rdfs:domain :Character ;
    rdfs:range :DevilFruit .

:race a owl:ObjectProperty ;
    rdfs:label "Race" ;
    rdfs:comment "The race of the character. e.g. Human, Fishman, Merfolk, Mink, Giant" ;
    rdfs:domain :Character ;
    rdfs:range :Race .

:inCrew a owl:ObjectProperty ;  
    rdfs:label "Crew" ;
    rdfs:comment "The crew to which this pirate belongs" ;
    owl:inverseOf :hasCrewMember ;
    rdfs:domain :Pirate ;
    rdfs:range :PirateCrew.

:placeOfOrigin a owl:ObjectProperty ;
    rdfs:label "Place of Origin"@en ;
    rdfs:comment "The place where a character is originally from" ;
    rdfs:domain :Character ;
    rdfs:range :Location .

:hasBounty a owl:ObjectProperty ;
    rdfs:label "Has Bounty"@en ;
    rdfs:comment "The bounty on a pirate's head in berries" ;
    rdfs:domain :Character ;
    rdfs:range :Bounty .

:ageAtDeath a owl:DatatypeProperty ;
    rdfs:label "Age at Death"@en ;
    rdfs:comment "The age of a character at the time of their death" ;
    rdfs:domain :Character ;
    rdfs:range xsd:integer .

:bountyAmount a owl:DatatypeProperty ;
    rdfs:label "Bounty Amount"@en ;
    rdfs:comment "The amount of the bounty in berries" ;
    rdfs:domain :Bounty ;
    rdfs:range xsd:integer .

:height a owl:DatatypeProperty ;
    rdfs:label "Height"@en;
    rdfs:comment "The height of a character in centimeters" ;
    rdfs:domain :Character ;
    rdfs:range xsd:integer .

:epithet a owl:DatatypeProperty ;
    rdfs:label "Epithet"@en;
    rdfs:comment "An epithet (通り名, Tōrina?) is a reputational title attached to a person, usually based on some well-known attribute—such as physical traits, personalities, or deeds." ;
    rdfs:domain :Character ;
    rdfs:range xsd:string .

### Arc Properties ###
:arcStartManga a owl:ObjectProperty ;
    rdfs:label "Arc Start Manga" ;
    rdfs:comment "The manga chapter in which the story arc starts" ;
    rdfs:domain :Arc ;
    rdfs:range :MangaIssue .

:arcEndManga a owl:ObjectProperty ;
    rdfs:label "Arc End Manga" ;
    rdfs:comment "The manga chapter in which the story arc ends" ;
    rdfs:domain :Arc ;
    rdfs:range :MangaIssue .

:arcStartAnime a owl:ObjectProperty ;
    rdfs:label "Arc Start Anime" ;
    rdfs:comment "The anime episode in which the story arc starts" ;
    rdfs:domain :Arc ;
    rdfs:range :AnimeEpisode .

:arcEndAnime a owl:ObjectProperty ;
    rdfs:label "Arc End Anime" ;
    rdfs:comment "The anime episode in which the story arc ends" ;
    rdfs:domain :Arc ;
    rdfs:range :AnimeEpisode .


### Pirate Crew Properties ###
:hasCrewMember a owl:ObjectProperty ;
    rdfs:label "Has Crew Member" ;
    rdfs:comment "A pirate belonging to this crew" ;
    owl:inverseOf :inCrew ;
    rdfs:domain :PirateCrew ;
    rdfs:range :Pirate .

### Devil Fruit Properties ###
:hasFruitType a owl:ObjectProperty ;
    rdfs:label "Devil Fruit Type"@en ;
    rdfs:comment "The type of devil fruit (Paramecia, Zoan, or Logia)" ;
    rdfs:domain :DevilFruit ;
    rdfs:range :DevilFruitType .

:DevilFruitCurrentUser a owl:ObjectProperty ;
    rdfs:label "Current User" ;
    rdfs:comment "The current user of the devil fruit" ;
    rdfs:domain :DevilFruit ;
    rdfs:range :Character .

:DevilFruitPreviousUser a owl:ObjectProperty ;
    rdfs:label "Previous User" ;
    rdfs:comment "A previous user of the devil fruit" ;
    rdfs:domain :DevilFruit ;
    rdfs:range :Character .

:devilFruitDebutManga a owl:DatatypeProperty ;
    rdfs:label "Devil Fruit Debut"@en ;
    rdfs:comment "The chapter or episode in which the devil fruit first appeared" ;
    rdfs:domain :DevilFruit ;
    rdfs:range :MangaIssue .

:devilFruitDebutAnime a owl:DatatypeProperty ;
    rdfs:label "Devil Fruit Debut Anime"@en ;
    rdfs:comment "The anime episode in which the devil fruit first appeared" ;
    rdfs:domain :DevilFruit ;
    rdfs:range :AnimeEpisode .

### Location Properties ###
:locatedIn a owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:label "Located In" ;
    rdfs:comment "The larger location in which this location is found" ;
    rdfs:domain :Location ;
    rdfs:range :Location .

### Bounty Properties ###
:givenAfter a owl:ObjectProperty ;
    rdfs:label "Given After"@en ;
    rdfs:comment "The arc after which the bounty was given" ;
    rdfs:domain :Bounty ;
    rdfs:range :Arc .
:givenTo a owl:ObjectProperty ;
    rdfs:label "Given To"@en ; 
    rdfs:comment "The character to whom the bounty was given" ;
    rdfs:domain :Bounty ;
    rdfs:range :Character .
